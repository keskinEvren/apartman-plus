{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "ASANMOD: Verify",
      "type": "shell",
      "command": "npm run verify",
      "group": "build",
      "problemMatcher": ["$eslint-compact", "$tsc"],
      "presentation": {
        "reveal": "always",
        "panel": "shared"
      }
    },
    {
      "label": "ASANMOD: Fix",
      "type": "shell",
      "command": "npm run fix",
      "group": "build",
      "problemMatcher": ["$eslint-compact"]
    },
    {
      "label": "ASANMOD: Status",
      "type": "shell",
      "command": "npm run status",
      "group": "none",
      "presentation": {
        "reveal": "always"
      }
    },
    {
      "label": "Dev: Start",
      "type": "shell",
      "command": "npm run dev",
      "isBackground": true,
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "DB: Generate",
      "type": "shell",
      "command": "npm run db:generate",
      "group": "none"
    },
    {
      "label": "DB: Migrate",
      "type": "shell",
      "command": "npm run db:migrate",
      "group": "none"
    }
  ]
}
